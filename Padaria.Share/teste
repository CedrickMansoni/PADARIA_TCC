server {
    listen 80;
    server_name mobile.gsaplatform.co;

   location / {
        proxy_pass http://127.0.0.1:8081; # Apontando para sua API na porta 8081
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

    }
}

